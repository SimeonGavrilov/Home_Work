@{
    Layout = null;
    //var me = @HttpContext.Current.User.Identity.Name
    var user = Request.QueryString["user"];
    if (IsPost)
    {
        Response.Redirect("/pages/SignOut.cshtml");
    }

}
<html lang="ru">
<head>
    <link href="~/Pages/Style.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.js"></script>
</head>
<body>
    <img src="~/Pages/Arts/4.jpg" alt="User img" class="img-thumbnail" id="PIC">
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="collapse navbar-collapse" id="navbarText">
            <ul class="navbar-nav mr-auto" id="MyNavBar">
                <li class="nav-item">
                    <a class="nav-link" href="~/Pages/Galery.cshtml?user=@user">Gallery</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="~/Pages/Favorite.cshtml?user=@user">Favorite</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="~/Pages/Latest Update.cshtml">Latest update</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="~/Pages/UserPage.cshtml?User=@HttpContext.Current.User.Identity.Name">My page</a>
                </li>
            </ul>
            <form method="post">
                <button class="btn btn-primary" id="Sign_Out_Button">Sign out</button>
            </form>

        </div>
    </nav>
    @RenderBody()
</body>
</html>